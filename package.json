{
  "name": "gazetracker",
  "version": "1.0.0",
  "description": "Eye tracking application using GazeCloud API",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "ts-node api/server.ts",
    "client": "cd client && npm start",
    "dev": "nodemon api/server.ts",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "d3-hexbin": "^0.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.19",
    "@types/d3": "^7.4.3",
    "@types/d3-hexbin": "^0.2.3",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.3",
    "concurrently": "^8.2.2"
  }
}
